{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Kestrel": {
        "EndpointDefaults": {
            "Protocols": "Http2"
        },
        "Endpoints": {
            "gRPC": {
                "Url": "http://+:9000"
            },
            "REST": {
                "Protocols": "Http1",
                "Url": "http://+:8080"
            }
        }
    }
}
